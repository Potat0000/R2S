config wifi-device 'radio0'
	option type 'mac80211'
	option path 'platform/ff5c0000.usb/usb2/2-1/2-1:1.0'
	option legacy_rates '0'
	option noscan '1'
	option mu_beamformer '0'
	option country '00'
	option disabled '0'
	option hwmode '11a'
	option channel '40'
	option htmode 'HT40'

config wifi-iface
	option device 'radio0'
	option mode 'ap'
	option network 'lan'
	option encryption 'psk2'
	option key 'password'
	option wps_pushbutton '0'
